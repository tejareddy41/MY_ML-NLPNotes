  "tokenColors": [
    { "scope": ["comment"], "settings": { "foreground": "#33ff33", "fontStyle": "italic" } },
    { "scope": ["keyword", "storage.type"], "settings": { "foreground": "#ff6600", "fontStyle": "bold" } },
    { "scope": ["variable", "string"], "settings": { "foreground": "#00ff00" } },
    { "scope": ["function", "entity.name.function"], "settings": { "foreground": "#00ff00" } },
    { "scope": ["constant.numeric", "constant.language"], "settings": { "foreground": "#ff3300" } },
    { "scope": ["class", "entity.name.type.class"], "settings": { "foreground": "#ff6600" } },
    { "scope": ["punctuation", "delimiter"], "settings": { "foreground": "#00ff00" } }
  ],

  "semanticTokenColors": {
    "variable.readonly": "#00ff00",
    "property": "#00ff00",
    "method": "#00ff00",
    "stringLiteral": "#00ff00",
    "numberLiteral": "#ff3300",
    "class": "#ff6600",
    "interface": "#00ff00",
    "enum": "#ff3300",
    "enumMember": "#ff3300"
  },

  "terminal.ansiBlack": "#0b0b0b",
  "terminal.ansiRed": "#ff3300",
  "terminal.ansiGreen": "#00ff00",
  "terminal.ansiYellow": "#ff6600",
  "terminal.ansiBlue": "#00ff00",
  "terminal.ansiMagenta": "#ff33ff",
  "terminal.ansiCyan": "#00ff00",
  "terminal.ansiWhite": "#33ff33",
  "terminal.ansiBrightBlack": "#111111",
  "terminal.ansiBrightRed": "#ff5555",
  "terminal.ansiBrightGreen": "#33ff33",
  "terminal.ansiBrightYellow": "#ff9933",
  "terminal.ansiBrightBlue": "#33ff33",
  "terminal.ansiBrightMagenta": "#ff66ff",
  "terminal.ansiBrightCyan": "#33ff33",
  "terminal.ansiBrightWhite": "#ffffff"
